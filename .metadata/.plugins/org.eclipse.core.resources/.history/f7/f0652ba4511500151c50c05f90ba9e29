package Qwertys;

public class Comunes {
	public String Select(String campos, String tabla){
		String qwerty="SELECT "+campos+" FROM "+tabla;
		return qwerty;
	}
	public String SelectWhere(String campos, String tabla, String comparador, String comparable){
		String qwerty="SELECT "+campos+" FROM "+tabla +" WHERE "+comparador+" = "+comparable;
		return qwerty;
	}
	public String insertInto(){
		String qwerty="INSERT INTO "
	}
}
