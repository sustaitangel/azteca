package Imagenes;

import javax.swing.Icon;
import javax.swing.ImageIcon;

public class Img {
	public Icon fondo(){
		Icon fondo= new ImageIcon(getClass().getResource("LogoAzteca.jpg"));
		return fondo;
	}
	public Icon login(){
		Icon fondo= new ImageIcon(getClass().getResource("azteca.png"));
		return fondo;
	}
}
